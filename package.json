{
  "name": "meu-app",
  "version": "1.0.0",
  "main": "index.js",
"scripts": {
  "start": "expo start",
  "android": "expo start --android",
  "ios": "expo start --ios",
  "web": "expo start --web",
  "start:dev": "cross-env APP_ENV=meu-app expo start",
  "start:rafael": "cross-env APP_ENV=rafael expo start",
  "start:marina": "cross-env APP_ENV=marina expo start",

  "publish:dev": "cross-env APP_ENV=meu-app eas update --branch dev --message \"Atualização DEV\"",
  "publish:rafael": "cross-env APP_ENV=rafael eas update --branch rafael --message \"Versão Rafael atualizada\"",
  "publish:marina": "cross-env APP_ENV=marina eas update --branch marina --message \"Versão Marina atualizada\""
},
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-masked-view/masked-view": "0.3.2",
    "@react-native-picker/picker": "2.11.1",
    "@react-navigation/bottom-tabs": "^7.4.5",
    "@react-navigation/material-top-tabs": "^7.3.7",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/native-stack": "^7.3.21",
    "d3-shape": "^3.2.0",
    "date-fns": "^4.1.0",
    "expo": "^54.0.12",
    "expo-constants": "~18.0.9",
    "expo-haptics": "~15.0.7",
    "expo-status-bar": "~3.0.8",
    "expo-updates": "~29.0.12",
    "firebase": "^12.0.0",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-pager-view": "6.9.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-tab-view": "^4.1.3",
    "react-native-vector-icons": "^10.3.0",
    "victory-native": "^41.19.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "cross-env": "^10.1.0"
  },
  "private": true
}
